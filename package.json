{
  "name": "concept-store-platform",
  "author": "Mak Tarsis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//...NG": "SECTION of angular scripts",
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "backend:serve": "cd backend && npm run dev",
    "//...ANALYZER & EXPLORER": "SECTION of build analyzer and explorer",
    "build:analyze:dev": "ng build --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:analyze:prod": "ng build --prod --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:explorer:dev": "ng build && npx source-map-explorer dist/concept-store-platform/vendor.js",
    "build:explorer:prod": "ng build --prod --source-map --output-hashing=none && npx source-map-explorer dist/concept-store-platform/main.js",
    "//...DOC": "SECTION of documentation",
    "doc:build": "compodoc -p src/typescript/tsconfig.app.json --theme=readthedocs",
    "doc:serve": "compodoc -s",
    "doc:build:serve": "compodoc -p src/typescript/tsconfig.app.json -s --theme=readthedocs -o"
  },
  "dependencies": {
    "@abraham/reflection": "^0.4.2",
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.5",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.5",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "6.1.3",
    "@angular/router": "^6.1.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "apollo-angular-boost": "^1.1.0",
    "graphql": "^14.0.0",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "^0.2.4",
    "raven-js": "^3.26.4",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.4",
    "@angular-devkit/build-optimizer": "^0.7.4",
    "@angular/cli": "^6.1.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.1.5",
    "@types/googlemaps": "^3.30.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.6.2",
    "codelyzer": "~4.2.1",
    "hoek": "^5.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.9.2",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
