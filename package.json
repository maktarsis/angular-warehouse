{
  "name": "angular-warehouse",
  "author": "Max Tarsis  <tarsis.maksym@gmail.com>",
  "description": "CSP is online store as a platform for consumers that loves conceptual clothing built on modern technologies and approaches",
  "version": "0.1.2",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maktarsis/concept-store-platform.git"
  },
  "bugs": "https://github.com/maktarsis/concept-store-platform/issues",
  "homepage": "https://github.com/maktarsis/concept-store-platform/blob/master/README.md",
  "scripts": {
    "// - NG - //": "------------------------------",
    "ng": "ng",
    "build": "ng build",
    "test": "snyk test && ng test",
    "e2e": "ng e2e",
    "// - SERVES - //": "------------------------------",
    "start": "ng serve",
    "hmr": "ng serve --configuration hmr",
    "server": "cd server && yarn start",
    "// - BUILD ANALYZERS & EXPLORERS - //": "------------------------------",
    "build:analyze:dev": "ng build --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:analyze:prod": "ng build --prod --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "deps-tree": "npx ostap ./package.json",
    "// - DOC - //": "------------------------------",
    "doc:build": "compodoc -p src/tsconfig.app.json --theme=readthedocs",
    "doc:serve": "compodoc -s",
    "doc:build:serve": "compodoc -p src/tsconfig.app.json -s --theme=readthedocs -o",
    "// - SNYK - //": "------------------------------",
    "snyk:test": "snyk test",
    "snyk:wizard": "snyk wizard",
    "// - LINTERS, FORMATTERS - //": "------------------------------",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier"
  },
