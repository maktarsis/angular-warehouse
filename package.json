{
  "name": "concept-store-platform",
  "author": "Mak Tarsis",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "//...NG": "SECTION of angular scripts",
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "backend:serve": "cd backend && npm run dev",
    "//...ANALYZER & EXPLORER": "SECTION of build analyzer and explorer",
    "build:analyze:dev": "ng build --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:analyze:prod": "ng build --prod --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:explorer:dev": "ng build && npx source-map-explorer dist/concept-store-platform/vendor.js",
    "build:explorer:prod": "ng build --prod --source-map --output-hashing=none && npx source-map-explorer dist/concept-store-platform/main.js",
    "//...DOC": "SECTION of documentation",
    "doc:build": "compodoc -p src/typescript/tsconfig.app.json --theme=readthedocs",
    "doc:serve": "compodoc -s",
    "doc:build:serve": "compodoc -p src/typescript/tsconfig.app.json -s --theme=readthedocs -o"
  },
  "dependencies": {
    "@abraham/reflection": "^0.4.2",
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/platform-server": "7.0.1",
    "@angular/router": "^7.0.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "apollo-angular-boost": "^1.3.0",
    "graphql": "^14.0.2",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "^0.2.4",
    "raven-js": "^3.27.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.6",
    "@angular-devkit/build-optimizer": "^0.10.3",
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.1.5",
    "@types/googlemaps": "^3.30.16",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "hoek": "^5.0.4",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "^0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^3.1.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
