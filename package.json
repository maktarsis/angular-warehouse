{
  "name": "concept-store-platform",
  "author": "Mak Tarsis  <tarsis.maksym@gmail.com>",
  "description": "CSP is online store as a platform for consumers that loves conceptual clothing built on modern technologies and approaches",
  "version": "0.1.2",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maktarsis/concept-store-platform.git"
  },
  "bugs": "https://github.com/maktarsis/concept-store-platform/issues",
  "homepage": "https://github.com/maktarsis/concept-store-platform/blob/master/README.md",
  "scripts": {
    "// - NG - //": "section of angular scripts",
    "ng": "ng",
    "build": "ng build",
    "test": "snyk test && ng test",
    "e2e": "ng e2e",
    "// - DEV SERVES - //": "section of scripts to run app in dev",
    "backend:serve": "cd backend && npm run dev",
    "frontend:serve": "ng serve",
    "frontend:hmr:serve": "ng serve --configuration hmr",
    "// - BUILD ANALYZERS & EXPLORERS - //": "SECTION of build analyzers and explorers",
    "build:analyze:dev": "ng build --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:analyze:prod": "ng build --prod --stats-json && npx webpack-bundle-analyzer dist/concept-store-platform/stats.json",
    "build:explorer:dev": "ng build && npx source-map-explorer dist/concept-store-platform/vendor.js",
    "build:explorer:prod": "ng build --prod --source-map --output-hashing=none && npx source-map-explorer dist/concept-store-platform/main.js",
    "// - DOC - //": "section of documentation",
    "doc:build": "compodoc -p src/typescript/tsconfig.app.json --theme=readthedocs",
    "doc:serve": "compodoc -s",
    "doc:build:serve": "compodoc -p src/typescript/tsconfig.app.json -s --theme=readthedocs -o",
    "// - SNYK - //": "section for https://app.snyk.io",
    "snyk:test": "snyk test",
    "snyk:wizard": "snyk wizard",
    "// - LINTERS, FORMATTERS - //": "section for linters and formatters",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier"
  },
