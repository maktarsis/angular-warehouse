{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
              "app",
              "auth",
              "cart",
              "core",
              "user-center",
              "shop",
              "location",
              "home"
            ],
            "kebab-case"
        ]
    }
}
